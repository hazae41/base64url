{
  "type": "module",
  "name": "@hazae41/base64url",
  "version": "1.0.5",
  "description": "Base64URL adapter for WebAssembly and JS implementations",
  "homepage": "https://github.com/hazae41/base64url",
  "repository": "github:hazae41/base64url",
  "author": "hazae41",
  "license": "MIT",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "./dist/esm",
    "./dist/cjs",
    "./dist/types"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "test": "node ./dist/test/index.test.mjs",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@hazae41/phobos": "^1.0.10",
    "@hazae41/rimraf": "^1.0.1",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.8.3",
    "rollup": "^4.0.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-node-externals": "^6.1.2",
    "typescript": "^5.2.2"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "keywords": [
    "base64url",
    "base64",
    "base-64",
    "encoding",
    "encoder",
    "adapter",
    "typescript",
    "esmodules"
  ],
  "dependencies": {
    "@hazae41/box": "^1.0.5",
    "@hazae41/option": "^1.0.25",
    "@hazae41/result": "^1.1.7"
  },
  "optionalDependencies": {
    "@hazae41/alocer": "^1.0.7",
    "@scure/base": "^1.1.3"
  }
}
